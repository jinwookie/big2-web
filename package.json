{
  "name": "big2-web",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src",
    "clean:server": "rimraf \"dist\"",
    "clean:client": "rimraf \"public\"",
    "clean": "npm run clean:server && npm run clean:client",
    "dev:server": "BABEL_ENV=node nodemon --exec babel-node src/server/app.js",
    "webpack:dev:server": "NODE_ENV=development BABEL_ENV=node webpack --config webpack/webpack.server.config.babel.js --watch",
    "webpack:dev:client": "NODE_ENV=development webpack-dev-server --config webpack/webpack.config.babel.js",
    "dev": "concurrently --kill-others \"npm run webpack:dev:server\" \"npm run webpack:dev:client\" \"npm run dev:start\"",
    "build:prod": "npm run clean && npm run webpack:prod:server && npm run webpack:prod:client",
    "webpack:prod:server": "NODE_ENV=production BABEL_ENV=node webpack --config webpack/webpack.server.config.babel.js",
    "webpack:prod:client": "NODE_ENV=production webpack --config webpack/webpack.config.babel.js --bail",
    "start": "NODE_ENV=production node dist/server/app.js",
    "dev:start": "nodemon dist/server/app.js",
    "postinstall": "npm run build:prod"
  },
  "author": "Brian",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-system-import-transformer": "^2.4.0",
    "babel-plugin-transform-require-ignore": "0.0.2",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-es2017": "^6.22.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-1": "^6.22.0",
    "bluebird": "^3.4.6",
    "concurrently": "^3.0.0",
    "css-loader": "^0.25.0",
    "dotenv": "^4.0.0",
    "eslint": "^3.14.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.9.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "font-awesome": "^4.7.0",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.4",
    "moment": "^2.17.0",
    "node-sass": "^3.10.0",
    "pug": "^2.0.0-beta6",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.6",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^1.0.0",
    "redux-immutable": "^3.0.11",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.4",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-node-externals": "^1.4.3"
  },
  "devDependencies": {
    "nodemon": "^1.10.2",
    "react-hot-loader": "^1.3.1",
    "webpack-dev-server": "^2.3.0"
  }
}
